{
	"name": "Flashgames maximizer",
	"version": "3.1.2.$Rev$",
	"description": "This script maximizes the flash of Farmville, Blackjack, Backyard Monsters, Country Life, Car Town, Cafe World, Farm Town, FishVille, FrontierVille, Path Words, PetVille, Roller Coaster Kingdom, Texas HoldEm Poker, Treasure Isle, Word Twist, YoVille and Zoo Paradise to the complete browser window. If a version of a game outside of facebook is available this is maximized too.",
	"minimum_chrome_version": "4.1.249.1045",
	"content_scripts": [
		{
			"all_frames": true,
			"js": [
				"Resources/jquery.gmfix.js",
				"Resources/jquery-ui.min.js",
				"Resources/fgm-styles.js",
				"Resources/fv-tools.js",
				"gm/substitute.js",
				"flashgames_maximizer.user.js"
			],
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"include_globs": [
				"*facebook.com/black_jack/*",
				"*facebook.com/backyardmonsters/*",
				"*fb.casualcollective.com/*",
				"*facebook.com/cartown/*",
				"*facebook.com/countrylife/*",
				"*countrylife.joyeurs.com/my_ranch/*",
				"*cartown.com/cartown/*",
				"*facebook.com/cafeworld/*",
				"*cafe.zynga.com/*",
				"*apps.facebook.com/farmtown/*",
				"*.slashkey.com/*",
				"*facebook.com/onthefarm/*",
				"*farmville.com/*",
				"*facebook.com/fishville/*",
				"*fishville.zynga.com/*",
				"*facebook.com/frontierville/*",
				"*frontier.zynga.com/*",
				"*facebook.com/pathwords/*",
				"*74.201.93.95/*",
				"*facebook.com/petvillegame/*",
				"*petville.zynga.com*",
				"*facebook.com/coasterkingdom/*",
				"*coaster.zynga.com*",
				"*facebook.com/texas_holdem/*",
				"*poker.zynga.com*",
				"*facebook.com/treasureisle/*",
				"*treasure.zynga.com/*",
				"*facebook.com/wordtwist/*",
				"*74.201.93.101*",
				"*facebook.com/yoville/*",
				"*yoville.com/*",
				"*apps.facebook.com/zoo-paradise/*",
				"*zoo.crowdstar.com/*"
			],
			"exclude_globs": [],
			"run_at": "document_end"
		}
	]
}